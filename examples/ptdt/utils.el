domain Utilities

export formula fromUnitToKilo(val: number) returns number {
    return val / 1000
}

export formula fromKiloToUnit(val: number) returns number {
    return val * 1000
}

export formula fromMegaToKilo(val: number) returns number {
    return val / 1000
}

export formula fromKiloToMega(val: number) returns number {
    return val * 1000
}

export formula fromUnitToMega(val: number) returns number {
    return val / 1000000
}

export formula fromMegaToUnit(val: number) returns number {
    return val * 1000000
}